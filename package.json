{
  "name": "reaxjs",
  "version": "0.4.0",
  "description": "react+rxjs",
  "main": "dist/main.js",
  "types": "lib/main.d.ts",
  "module": "lib/main.js",
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d dist lib",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusGundersen/reaxjs.git"
  },
  "keywords": [
    "react",
    "rxjs"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/reaxjs/issues"
  },
  "homepage": "https://github.com/mariusGundersen/reaxjs#readme",
  "peer-dependencies": {
    "react": "^16.3.2",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.3.14",
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "react": "^16.3.2",
    "rxjs": "^6.1.0",
    "typescript": "^2.8.3"
  },
  "ava": {
    "files": [
      "lib/**/*.test.js",
      "src/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
