{
  "name": "reaxjs",
  "version": "0.5.0",
  "description": "react+rxjs",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "clean": "rimraf ./es ./js",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d js es",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusGundersen/reaxjs.git"
  },
  "keywords": [
    "react",
    "rxjs"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/reaxjs/issues"
  },
  "homepage": "https://github.com/mariusGundersen/reaxjs#readme",
  "peer-dependencies": {
    "react": "^16.3.2",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/node": "^10.0.0",
    "@types/react-dom": "^16.8.5",
    "ava": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "ava": {
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ]
  }
}